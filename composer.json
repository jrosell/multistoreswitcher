{
    "name": "jrosell/multistoreswitcher",
    "type": "prestashop-module",
    "description": "A lightweight multistore switcher for PrestaShop 9. Allows users to switch between shops in multistore setups.",
    "keywords": ["prestashop", "module", "multistore", "switcher", "shop selector"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jordi Rosell",
            "email": "jroselln@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.85",
        "prestashop/php-dev-tools": "^5.0",
        "prestashop/header-stamp": "^2.6"
    },
    "autoload": {
        "files": ["multistoreswitcher.php"]
    },
    "extra": {
        "installer-name": "multistoreswitcher"
    },
    "config": {
        "prepend-autoloader": false,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}